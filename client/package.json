{
  "name": "WillingQuiz",
  "version": "2.17.15",
  "private": true,
  "scripts": {
    "bump": "npm version -m 'Bump version to %s.'",
    "check": "flow && eslint src __tests__ && jsinspect src && yarn test",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "fix": "eslint --fix *.js src __tests__",
    "fix-check": "yarn fix && flow && jsinspect src && yarn test",
    "postinstall": "rndebugger-open",
    "precommit": "lint-staged",
    "prettier": "prettier --write {src,scripts,__tests__,../server}/**/*.{js,json,ts} *.{js,json,ts}",
    "format":
      "yarn sort-comp && rr '\"sort-imports\": \"off\"' '\"sort-imports\": \"warn\"' .eslintrc.json; yarn prettier; yarn fix; rr '\"sort-imports\": \"warn\"' '\"sort-imports\": \"off\"' .eslintrc.json",
    "sort-comp": "jscodeshift --parser=flow -t scripts/react-codemod/sort-comp.js src __tests__",
    "start": "react-native start",
    "test": "jest"
  },
  "rnpm": {
    "assets": ["fonts"]
  },
  "dependencies": {
    "babel-plugin-transform-remove-console": "^6.9.0",
    "fbemitter": "^2.1.1",
    "immutable": "4.0.0-rc.7",
    "lodash": "^4.17.13",
    "moment": "^2.20.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.48.4",
    "react-native-code-push": "^5.2.2",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-modal": "^5.2.0",
    "react-native-pathjs-charts": "0.0.31",
    "react-native-photo-view": "^1.5.2",
    "react-native-push-notification": "^3.0.2",
    "react-native-radio-buttons": "^1.0.0",
    "react-native-root-toast": "^3.0.1",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-snap-carousel": "^3.6.0",
    "react-native-svg": "5.4.2",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.51.0",
    "flow-typed": "^2.3.0",
    "husky": "^0.14.3",
    "jest": "22.4.2",
    "jest-enzyme": "^5.0.3",
    "jscodeshift": "^0.5.0",
    "jsinspect": "^0.12.7",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react-dom": "16.0.0-alpha.12",
    "react-native-debugger-open": "^0.3.17",
    "react-test-renderer": "16.0.0-alpha.12",
    "rexreplace": "^3.0.1"
  }
}
